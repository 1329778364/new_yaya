<view style="background-color:rgb(237,237,237);"><form style="margin:10px;"><view class="cu-form-group"><view class="title">姓名</view><input placeholder="请输入您的名字" id="userName" data-event-opts="{{[['input',[['commonInput',['$event']]]]]}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">身份证号</view><input type="idcard" placeholder="请输入您的身份证号" id="idCard" data-event-opts="{{[['input',[['commonInput',['$event']]]]]}}" bindinput="__e"/></view><view class="cu-bar bg-white"><view class="action">{{''+(registerType=="校园帮"?"请上传学生证/身份证正反面":"请上传身份证正反面")+''}}</view><view class="action">{{''+commonRegisterInfo.imgList.length+"/ "+(registerType=="校园帮"?4:2)+''}}</view></view><view class="cu-form-group"><view class="grid col-4 grid-square flex-sub"><block wx:for="{{commonRegisterInfo.imgList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="bg-img" data-url="{{imgList[index]}}" data-event-opts="{{[['tap',[['ViewImage',['$event']]]]]}}" bindtap="__e"><image src="{{commonRegisterInfo.imgList[index]}}" mode="aspectFill"></image><view class="cu-tag bg-red" data-index="{{index}}" data-event-opts="{{[['tap',[['DelImg',['$event']]]]]}}" catchtap="__e"><text class="cuIcon-close"></text></view></view></block><block wx:if="{{commonRegisterInfo.imgList.length<imgNum}}"><view data-event-opts="{{[['tap',[['ChooseImage',['$event']]]]]}}" class="solids" bindtap="__e"><text class="cuIcon-cameraadd"></text></view></block></view></view><block wx:if="{{schoolShow}}"><view class="cu-form-group"><view class="title">目前学历</view><picker value="{{index_education}}" range="{{educationPicker}}" data-event-opts="{{[['change',[['selectEducation',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(index_education>-1?educationPicker[index_education]:"请选择...")+''}}</view></picker></view></block><block wx:if="{{schoolShow}}"><view data-event-opts="{{[['tap',[['showSchoolPicker',['$event']]]]]}}" class="cu-form-group" bindtap="__e"><view class="title">院校信息</view><view class="picker">{{''+studentRegisterInfo.provinceSchool+''}}</view></view></block><school-picker class="vue-ref" vue-id="1" themeColor="#000" data-ref="schoolPicker" data-event-opts="{{[['^onConfirm',[['onConfirm']]]]}}" bind:onConfirm="__e" bind:__l="__l"></school-picker><block wx:if="{{schoolShow}}"><view class="cu-form-group"><view class="title">学院</view><input id="college" placeholder="建议填写学校官方的学院全称" data-event-opts="{{[['input',[['commonInput',['$event']]]]]}}" bindinput="__e"/></view></block><block wx:if="{{schoolShow}}"><view class="cu-form-group"><view class="title">专业方向</view><input id="major" placeholder="将会优先为您匹配合适的人选或任务" data-event-opts="{{[['input',[['commonInput',['$event']]]]]}}" bindinput="__e"/></view></block><view class="cu-form-group"><view class="title">手机号码</view><input type="number" id="phoneNumber" placeholder="用于及时跟您进行联系" data-event-opts="{{[['input',[['commonInput',['$event']]]]]}}" bindinput="__e"/><button data-event-opts="{{[['tap',[['sendValCode',['$event']]]]]}}" class="cu-btn bg-green shadow" bindtap="__e">验证码</button></view><view class="cu-form-group"><view class="title">验证码</view><input type="number" id="validateCode" placeholder="输入之后才能提交信息!" data-event-opts="{{[['input',[['commonInput',['$event']]]]]}}" bindinput="__e"/></view><view class="cu-form-group margin-top-sm"><button data-event-opts="{{[['tap',[['submitAllInformation',['$event']]]]]}}" class="cu-btn bg-green" style="margin:0 auto;" bindtap="__e">提交信息</button></view><view class="{{['cu-modal',submitErrModal?'show':'']}}"><view class="cu-dialog"><view class="cu-bar bg-white justify-end"><view class="content">{{modalTitle}}</view><view data-event-opts="{{[['tap',[['hideSubmitErrModal',['$event']]]]]}}" class="action" bindtap="__e"><text class="cuIcon-close text-red"></text></view></view><view class="padding-xl">{{''+modalContent+''}}</view></view></view></form></view>